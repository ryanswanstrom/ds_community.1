<div class="row">
  <div class="col-md-12">
    <h1>Welcome to Data Science Community</h1>
    
    
    <!-- Begin MailChimp Signup Form -->
<link href="//cdn-images.mailchimp.com/embedcode/slim-081711.css" rel="stylesheet" type="text/css">
<div id="mc_embed_signup">
<form action="<%='//community.us7.list-manage.com/subscribe/post?u=' + (Rails.env.production? ? ENV['MC_KEY'] : 'abc') + '&amp;id=' + (Rails.env.production? ? ENV['MC_ID'] : 'abc')%>" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
	<label for="mce-EMAIL">Data Science Community Newsletter</label>
	<input type="email" value="" name="EMAIL" class="email" id="mce-EMAIL" placeholder="email address" required>
    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
    <div style="position: absolute; left: -5000px;"><input type="text" name="<%='b_' + (Rails.env.production? ? ENV['MC_KEY'] : 'abc') + '_' + (Rails.env.production? ? ENV['MC_ID'] : 'abc')%>" tabindex="-1" value=""></div>
    <div class="clear"><input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="button"></div>
</form>
</div>

<!--End mc_embed_signup-->
  </div>
</div>
